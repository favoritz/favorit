.page {
  
  background: #f2f2f2;
  min-height: 100%;
  max-height: 100%;
}
.swiper-container{
  width: 750rpx;
  position: relative;
}

swiper-item image {
  width: 100%;
  display: inline-block;
  overflow: hidden;
  height:400rpx;

}
.swiper-container .dots{  
  position: absolute;  
  left: 0;  
  right: 0;  
  bottom: 20rpx;  
  display: flex;  
  justify-content: center;  
}  
.swiper-container .dots .dot{  
  margin: 0 8rpx;  
  width: 14rpx;  
  height: 14rpx;  
  background: #fff;  
  border-radius: 50%;  
  transition: all .6s;  
  opacity: .5;
}  
.swiper-container .dots .dot.active{  
  width: 14rpx;  
  opacity: 1;
}



