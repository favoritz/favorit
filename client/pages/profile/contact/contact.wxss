/* pages/profile/contact/contact.wxss */
button{
  display:flex;
  font-size:40rpx;
  margin-bottom:0rpx;
  color:#555555;
}
.my{
  display:flex;
  flex-direction: column;
  margin-top:45rpx;
  margin-bottom:45rpx;
}
.my .contact{
  background-color: white;
  display:flex;
  height:100rpx; 
  line-height:100rpx;
  color:#555555; 
  font-weight:600; 
  align-items: center;
  border-bottom: 4rpx solid #f3f3f3;
}

.my .contact text{
  display:flex;
  justify-content: flex-start;
  padding-left:30rpx;
  align-items: center;
  vertical-align:middle;
}

.my .contact input{
  display:flex;
  justify-content: flex-start;
  padding-left:30rpx;
  align-items: center;
  vertical-align:middle;
}
.my button{
  margin-top:100rpx; 
  margin-left:30rpx;
  margin-right: 30rpx;
  justify-content: center;
  background-color:#0aacbb;
  color:white;

}
